.section init
    .temps r6, r7
    .zero r0
    .space 500
    endstack:
    r2 := endstack

.section text

    r3 := 0
    r4 := 400000000

    loop:
        if (r3 == r4) goto stop using r5
        push r3 on stack r2
        goto printd linking r1
        output "\n"
        pop r3 off stack r2
        r3 := r3 + 1
        goto loop using r5

    
    stop:
        halt





